{
	"name": "notes",
	"version": "1.0.0",
	"scripts": {
		"dev": "vite dev",
		"build": "prisma generate && prisma migrate deploy && vite build",
		"build:tsc": "tsc --noEmit",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run && npm run check",
		"test:unit": "vitest",
		"test:playwright": "playwright test",
		"lint": "prettier . --check . && eslint .",
		"lint-staged": "lint-staged",
		"format": "prettier . --write .",
		"prepare": "husky",
		"postinstall": "prisma generate"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.12.0",
		"@faker-js/faker": "^10.1.0",
		"@fontsource/fira-mono": "^5.0.8",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.0",
		"@neoconfetti/svelte": "^2.0.0",
		"@playwright/test": "^1.37.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-vercel": "^6.0.0",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/postcss": "^4.1.8",
		"@tailwindcss/typography": "^0.5.15",
		"@types/jsonwebtoken": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^8.8.1",
		"@typescript-eslint/parser": "^8.8.1",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^10.0.0",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"husky": "^9.0.7",
		"lint-staged": "^16.0.0",
		"postcss": "^8.4.28",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.7.0",
		"prisma": "^6.0.0",
		"svelte": "^5.16.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.1.8",
		"tslib": "^2.6.2",
		"typescript": "^5.5.0",
		"vite": "^6.0.0",
		"vitest": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@auth0/auth0-spa-js": "^2.1.1",
		"@aws-sdk/client-ses": "^3.540.0",
		"@prisma/client": "^6.0.0",
		"@tiptap/core": "^3.0.0",
		"@tiptap/extension-placeholder": "^3.0.0",
		"@tiptap/pm": "^3.0.0",
		"@tiptap/starter-kit": "^3.0.0",
		"fp-ts": "^2.16.5",
		"jsonwebtoken": "^9.0.1",
		"jwks-rsa": "^3.0.1",
		"nanoid": "^5.0.1",
		"prisma-field-encryption": "^1.5.0",
		"zod": "^4.0.0"
	},
	"lint-staged": {
		"*": [
			"prettier . --write",
			"eslint ."
		]
	},
	"engines": {
		"node": "22.x"
	}
}
